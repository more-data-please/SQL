---- This query was used to calculate the overall amount spent on Uber Eats ----
SELECT
SUM(total) AS total_spent

FROM
orders;


---- This query was used to calculate how much was spent in 2023 ----
SELECT
SUM(total)

FROM
orders

WHERE
order_date
BETWEEN '2023-01-01' AND '2023-12-31';


---- This query was used to calculate how much was spent in 2024 ----
SELECT 
SUM(total)

FROM
orders

WHERE
order_date
BETWEEN '2024-01-01' AND '2024-12-31';


---- This query was used to calculate how much was spent in 2025 so far ----
SELECT 
SUM(total)

FROM 
orders

WHERE 
order_date 
BETWEEN '2025-01-01' AND '2025-12-31';


---- This query was used to calculate the maximum amount spent on a tip an order ----
SELECT 
MAX(tip) AS max_tip

FROM
orders;

---- This query was used to calculate how much was spent at each restaurant ----
SELECT 
name,
SUM(total) AS total

FROM
orders

GROUP BY
name

ORDER BY
total DESC;


---- This query was used to calculate the amount of orders at each restaurant ----
SELECT 
name,
COUNT(order_id) AS total_orders

FROM
orders

GROUP BY
name

ORDER BY
total_orders;
