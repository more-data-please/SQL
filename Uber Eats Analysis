---- The following query was used to create a table of all orders placed on Uber Eats since 2023 ----
CREATE TABLE orders (
    order_id INT,
    name VARCHAR(255),
    type VARCHAR(255),
    subtotal DECIMAL,
    service_fee DECIMAL,
    tax DECIMAL,
    delivery_fee DECIMAL,
    tip DECIMAL,
    total DECIMAL,
    membership_benefit VARCHAR(3),
    discount VARCHAR(3),
    discount_amount DECIMAL,
    order_date DATE,
    order_time TIME,
	PRIMARY KEY (order_id)
);


